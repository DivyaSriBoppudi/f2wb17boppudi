extends layout

block content
  h1= title
  - var image_names = ["candies.jpg","candy_corn.jpg","candy.jpg","smarties.jpg","tootsie_roll.jpg"];
  - for(i=0;i<3;i++) {
     - var random = Math.floor(Math.random()*5) ;
     img(src=`./images/${image_names[random]}`, alt=`img${image_names[random]}`, style='width:300px; height:300px;')
  -}
  